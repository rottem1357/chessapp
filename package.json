{
  "name": "chessapp",
  "version": "1.0.0",
  "description": "Chess application similar to chess.com",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "predev": "node scripts/ensure-redis.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "postdev": "node scripts/stop-redis.js",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "chess",
    "react",
    "nodejs",
    "websocket",
    "game"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.0",
    "express-validator": "^7.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  }
}
