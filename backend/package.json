{
  "name": "chessapp-backend",
  "version": "1.0.0",
  "description": "Backend for chess application - Provides real-time multiplayer chess games and AI opponents",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest tests/auth --detectOpenHandles --forceExit",
    "test:games": "jest tests/games --detectOpenHandles --forceExit",
    "test:users": "jest tests/users --detectOpenHandles --forceExit",
    "test:puzzles": "jest tests/puzzles --detectOpenHandles --forceExit",
    "test:friends": "jest tests/friends --detectOpenHandles --forceExit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs": "node scripts/generateDocs.js"
  },
  "keywords": [
    "chess",
    "backend",
    "nodejs",
    "websocket",
    "socket.io",
    "express",
    "real-time",
    "multiplayer",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chess.js": "^1.0.0-beta.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.5.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/chessapp.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/chessapp/issues"
  },
  "homepage": "https://github.com/your-username/chessapp#readme"
}
