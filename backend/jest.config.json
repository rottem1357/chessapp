{
  "testEnvironment": "node",
  "verbose": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/tests/**",
    "!**/coverage/**",
    "!server.js"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "setupFilesAfterEnv": [],
  "testTimeout": 10000
}
